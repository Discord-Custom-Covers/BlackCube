// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// After the database connection

model User {
  id Int @id @default(autoincrement())
  uid String @unique
  img String
  orientation String
}